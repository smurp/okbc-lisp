%%%%
%%%%  Compressed item list (items are spaced closer together):
\newcommand{\bCitem}
	   {\begin{list}
	    {$\bullet$}
	    {\itemsep 0.01in \parsep 0.01in \topsep .1in}}
%%%%  \partopsep .01in \parskip .01in
\newcommand{\eCitem}{\end{list}}

\def\picture #1 by #2 (#3) indent #4 {
   \moveright #4
   \vbox to #2{
      \hrule width #1 height 0pt depth 0pt
      \vfill
      \special{picture #3}}}

\def\scaledpicture #1 by #2 (#3) indent #4 scale #5 {
  \dimen0=#1  \dimen1=#2
  \divide\dimen0 by 1000 \multiply\dimen0 by #5
  \divide\dimen1 by 1000 \multiply\dimen1 by #5
  {\picture \dimen0 by \dimen1 (#3 scaled #5) indent #4 }}

%%%%
%%%%  Narrows the left margin and the line spacing
%%%%
\newenvironment{scrunch}%
   {\setlength{\baselineskip}{.5\baselineskip}
    \begin{list}{}{\leftmargin 3em}
    \item
   }%
   {\setlength{\baselineskip}{2.\baselineskip}
    \end{list}
   }

